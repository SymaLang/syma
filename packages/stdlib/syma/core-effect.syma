{Module
  Core/Effect
  {Export Eff}
  {Rules
    {R Eff
      {.. {Eff ef_} ..}
      {Splat {Eff ef_} {.. ..}}
      :scope Program
    }

    {R Eff->Pending
      {Program {Eff ef_} .. {Effects {Pending ..} ..} ..}
      {Program .. {Effects {Pending .. ef_} ..} ..}
    }

    {R Eff/MoveLeft
      {Program .. {Eff e..} ..}
      {Program {Eff e..} .. ..}
    }
  }
}
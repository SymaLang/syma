{
  "name": "syma-fe",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "bin": {
    "syma": "./bin/syma-repl.js",
    "syma-repl": "./bin/syma-repl.js",
    "syma-compile": "./bin/syma-compile.js",
    "symc": "./bin/syma-compile.js"
  },
  "scripts": {
    "repl": "node bin/syma-repl.js",
    "build:parser": "cd tree-sitter-syma && npm run generate && npm run build-wasm",
    "build:parser:node": "cd tree-sitter-syma && npm run build",
    "test:parser": "cd tree-sitter-syma && npm test",
    "build:stdlib": "node bin/build-stdlib.js",
    "dev": "vite",
    "dev:demo": "VITE_DEMO_MODE=true vite",
    "build": "npm run build:stdlib && vite build",
    "preview": "vite preview",
    "compile": "node bin/syma-compile.js src/modules/*.syma --bundle --entry App/Main --out public/universe.json --pretty",
    "watch": "nodemon --watch 'src/modules/**/*.syma' --ext syma --exec 'npm run compile'",
    "demo:counter": "VITE_SYMA_ENTRY=src/demos/counter.syma npm run dev",
    "demo:todo": "VITE_SYMA_ENTRY=src/demos/todo/todo.syma npm run dev",
    "demo:vm": "VITE_SYMA_ENTRY=src/demos/vm/vm.syma npm run dev",
    "demo:effects": "VITE_SYMA_ENTRY=src/demos/effects-demo.syma npm run dev",
    "demo:bf": "VITE_SYMA_ENTRY=src/demos/bf.syma npm run dev",
    "demo:math": "VITE_SYMA_ENTRY=src/demos/math.syma npm run dev",
    "demo:print": "VITE_SYMA_ENTRY=src/demos/print-demo.syma npm run dev",
    "demo:input": "VITE_SYMA_ENTRY=src/demos/input-demo.syma npm run dev",
    "build:demo:counter": "node bin/syma-compile.js src/demos/counter.syma --bundle --out public/universe.json --pretty",
    "build:demo:todo": "node bin/syma-compile.js src/demos/todo/todo.syma --bundle --out public/universe.json --pretty",
    "build:demo:effects": "node bin/syma-compile.js src/demos/effects-demo.syma --bundle --out public/universe.json --pretty",
    "build:demo:bf": "node bin/syma-compile.js src/demos/bf.syma --bundle --out public/universe.json --pretty",
    "build:demo:math": "node bin/syma-compile.js src/demos/math.syma src/stdlib/algebra-simplify.syma --bundle --entry Demo/Math --out public/universe.json --pretty",
    "build:demo:print": "node bin/syma-compile.js src/demos/print-demo.syma --bundle --out public/universe.json --pretty",
    "build:demo:input": "node bin/syma-compile.js src/demos/input-demo.syma --bundle --out public/universe.json --pretty"
  },
  "devDependencies": {
    "@rollup/pluginutils": "^5.3.0",
    "@vitejs/plugin-react": "^5.0.3",
    "tree-sitter-cli": "^0.25.9",
    "vite": "^7.1.2"
  },
  "dependencies": {
    "@floating-ui/react": "^0.27.16",
    "@heroicons/react": "^2.2.0",
    "@monaco-editor/react": "^4.7.0",
    "@tailwindcss/vite": "^4.1.13",
    "clsx": "^2.1.1",
    "glob": "^11.0.3",
    "katex": "^0.16.22",
    "marked": "^16.3.0",
    "monaco-editor": "^0.53.0",
    "nanoid": "^5.1.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-markdown": "^10.1.0",
    "rehype-katex": "^7.0.1",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0",
    "tailwindcss": "^4.1.13",
    "tree-sitter": "^0.25.0",
    "web-tree-sitter": "^0.25.9",
    "zustand": "^5.0.8"
  }
}

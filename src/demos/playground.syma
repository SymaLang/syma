{Module
  Demo/Playground
  {Import Core/Fun as CF open}
  {Program}
  {Rules
    {Def Fib    {Args n_}       Call(FibAcc, n_, 0, 1)}
    {Def FibAcc {Args 0 a_ _}   a_}
    {Def FibAcc {Args n_ a_ b_} Call(FibAcc, {- n_ 1}, b_, {+ a_ b_}) :guard {> n_ 0}}

    {.some-class {
      color: red;
    }}
  }
}
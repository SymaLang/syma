{Module
  Demo/Print

  {Import Core/Plumb as CP open}
  {Import Core/Effect as CE open}

  {Program
    App(
      State(Empty),
      {UI Div("Check console for print effect")}
    )
    {EffQueue
      {Print Format("Initial print command")}
      {Print Format("Second print command")}
    }
  }
  {Rules
    R(
      "Format/Message",
      {Format m_},
      Concat("Printed message: ", m_)
    )
  }
}
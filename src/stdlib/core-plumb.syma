{Module
  Core/Plumb
  {Export Plumb PlumbEnd}
  {RuleRules
    R(
      "PlumbThroughProgram",
      R(SYMARULENAME_, {Plumb acts...}, {PlumbEnd endacts...}),
      R(
        SYMARULENAME_,
        {Program {/! pre...} acts... {/! post...}},
        {Program {/! pre...} endacts... {/! post...}}
      )
    )
  }
}
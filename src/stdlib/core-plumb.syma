{Module
  Core/Plumb
  {Export Plumb PlumbEnd}
  {RuleRules
    R(
      "PlumbThroughProgram",
      R(SYMARULENAME_, {Plumb SYMAACTS...}, {PlumbEnd SYMAENDACTS...}, SYMARULEREST...),
      R(
        SYMARULENAME_,
        {Program {/! pre...} SYMAACTS... {/! post...}},
        {Program {/! pre...} SYMAENDACTS... {/! post...}},
        SYMARULEREST...
      )
    )
  }
}